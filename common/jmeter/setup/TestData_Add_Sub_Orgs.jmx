        <BeanShellPreProcessor guiclass="TestBeanGUI" testclass="BeanShellPreProcessor" testname="BeanShell PreProcessor" enabled="true">
          <boolProp name="resetInterpreter">false</boolProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="script">String subOrgNamePrefix = vars.get(&quot;subOrgNamePrefix&quot;);
String subOrgName = &quot;${subOrgNamePrefix}${sub_org_idx}&quot;;
String description = &quot;This is the sub organization ${subOrgNamePrefix}${sub_org_idx}.&quot;;

vars.put(&quot;subOrgName&quot;, new String(subOrgName));
vars.put(&quot;suOrgDescription&quot;, new String(description));
</stringProp>
        </BeanShellPreProcessor>
