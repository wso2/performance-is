          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="script">import java.util.concurrent.ConcurrentLinkedQueue

if (ctx.getThreadNum() == 0) { // Only one thread writes
    def queue = props.get(&quot;tokenQueue&quot;) as ConcurrentLinkedQueue
    if (queue != null) {
        def outputFile = new File(&quot;/tmp/refresh_tokens.csv&quot;)

        if (outputFile.exists()) {
            outputFile.delete()
            log.info(&quot;Old refresh_tokens.csv file deleted.&quot;)
        }

        def tokenCount = queue.size()

        try {
            // Write header and data
            outputFile.text = &quot;refresh_token,client_id,client_secret\n&quot;
            queue.each { row -&gt;
                outputFile.append(row + &quot;\n&quot;)
            }

            log.info(&quot;Wrote ${tokenCount} refresh tokens to: &quot; + outputFile.getAbsolutePath())
        } catch (Exception e) {
            log.error(&quot;Failed to write tokens to file&quot;, e)
        }

        // Cleanup
        queue.clear()
        props.remove(&quot;tokenQueue&quot;)
        log.info(&quot;tokenQueue cleared from memory&quot;)
    } else {
        log.warn(&quot;tokenQueue not found â€” nothing to write.&quot;)
    }
}</stringProp>
